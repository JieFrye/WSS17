Cell[TextData[{
 "From the central limit theorem, the sampling distribution of the sample \
mean is approximated by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", "(", 
    RowBox[{"\[Mu]", ",", 
     RowBox[{"\[Sigma]", "/", 
      SqrtBox["n"]}]}], ")"}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"83f0c2e9-51ed-446e-b71d-4fd5f00a125d"],
 " for infinite populations. In the finite population case, the standard \
deviation is reduced slightly due to the finite-sample correction factor."
}], "Text",
 CellChangeTimes->{
  3.707495372783062*^9},ExpressionUUID->"cc3d9c5f-8c7f-4e52-a66e-\
c7e54e250bf7"],

Cell[BoxData[
 TagBox[
  StyleBox[
   DynamicModuleBox[{$CellContext`NREP$$ = 1000, $CellContext`nSize$$ = 
    35, $CellContext`POP$$ = 1, $CellContext`sr$$ = 
    2977, $CellContext`STAT$$ = 1, Typeset`show$$ = True, 
    Typeset`bookmarkList$$ = {}, Typeset`bookmarkMode$$ = "Menu", 
    Typeset`animator$$, Typeset`animvar$$ = 1, Typeset`name$$ = 
    "\"untitled\"", Typeset`specs$$ = {{{
       Hold[$CellContext`POP$$], 1, "population"}, {
      1 -> "finite", 2 -> "normal", 3 -> "uniform", 4 -> "Laplace", 5 -> 
       "gamma"}}, {{
       Hold[$CellContext`STAT$$], 1, "statistic"}, {
      1 -> "mean", 2 -> "standard deviation"}}, {{
       Hold[$CellContext`nSize$$], 5, "sample size"}, 5, 60, 5}, {{
       Hold[$CellContext`NREP$$], 1000, "number of samples"}, {1000, 5000, 
      10000}}, {{
       Hold[$CellContext`sr$$], 212, "random seed"}, 1, 12345, 1}}, 
    Typeset`size$$ = {315., {182., 189.}}, Typeset`update$$ = 0, 
    Typeset`initDone$$, Typeset`skipInitDone$$ = 
    False, $CellContext`POP$229$$ = False, $CellContext`STAT$234$$ = 
    False, $CellContext`nSize$235$$ = 0, $CellContext`NREP$236$$ = 
    0, $CellContext`sr$237$$ = 0}, 
    DynamicBox[Manipulate`ManipulateBoxes[
     1, StandardForm, 
      "Variables" :> {$CellContext`NREP$$ = 1000, $CellContext`nSize$$ = 
        5, $CellContext`POP$$ = 1, $CellContext`sr$$ = 
        212, $CellContext`STAT$$ = 1}, "ControllerVariables" :> {
        Hold[$CellContext`POP$$, $CellContext`POP$229$$, False], 
        Hold[$CellContext`STAT$$, $CellContext`STAT$234$$, False], 
        Hold[$CellContext`nSize$$, $CellContext`nSize$235$$, 0], 
        Hold[$CellContext`NREP$$, $CellContext`NREP$236$$, 0], 
        Hold[$CellContext`sr$$, $CellContext`sr$237$$, 0]}, 
      "OtherVariables" :> {
       Typeset`show$$, Typeset`bookmarkList$$, Typeset`bookmarkMode$$, 
        Typeset`animator$$, Typeset`animvar$$, Typeset`name$$, 
        Typeset`specs$$, Typeset`size$$, Typeset`update$$, Typeset`initDone$$,
         Typeset`skipInitDone$$}, "Body" :> 
      Module[{$CellContext`f$, $CellContext`\[Sigma]$, $CellContext`\[Mu]$, \
$CellContext`PR$, $CellContext`PLS$, $CellContext`S$, $CellContext`FPC$, \
$CellContext`gN$, $CellContext`gS$, $CellContext`epi$, $CellContext`GS$}, 
        SeedRandom[$CellContext`sr$$]; 
        Switch[$CellContext`STAT$$, 
          1, $CellContext`f$ = 
           Mean; $CellContext`\[Sigma]$ = $CellContext`SDX/
            Sqrt[$CellContext`nSize$$]; $CellContext`\[Mu]$ = $CellContext`MX; \
$CellContext`PR$ = $CellContext`PRMean; $CellContext`PLS$ = Style[
             StringForm[
             "histogram of sample means of samples of size ``", \
$CellContext`nSize$$], "Label", 10, Bold], 
          2, $CellContext`f$ = 
           StandardDeviation; $CellContext`PR$ = $CellContext`PRSD; \
$CellContext`PLS$ = Style[
             StringForm[
             "histogram of sample SD of samples of size ``", \
$CellContext`nSize$$], "Label", 10, Bold]]; $CellContext`S$ = 
         Which[$CellContext`POP$$ == 1, 
           Table[
            $CellContext`f$[
             
             RandomSample[$CellContext`X, $CellContext`nSize$$]], \
{$CellContext`NREP$$}], $CellContext`POP$$ == 2, 
           Table[
            $CellContext`f$[
             RandomReal[
              
              NormalDistribution[$CellContext`MX, $CellContext`SDX], \
$CellContext`nSize$$]], {$CellContext`NREP$$}], $CellContext`POP$$ == 3, 
           Table[
            $CellContext`f$[
             RandomReal[
              
              UniformDistribution[{$CellContext`A, $CellContext`B}], \
$CellContext`nSize$$]], {$CellContext`NREP$$}], $CellContext`POP$$ == 4, 
           Table[
            $CellContext`f$[
             RandomReal[
              
              LaplaceDistribution[$CellContext`MX, $CellContext`L\[Beta]], \
$CellContext`nSize$$]], {$CellContext`NREP$$}], $CellContext`POP$$ == 5, 
           Table[
            $CellContext`f$[
             RandomReal[
              
              GammaDistribution[$CellContext`Galpha, $CellContext`Gbeta], \
$CellContext`nSize$$]], {$CellContext`NREP$$}]]; {$CellContext`\[Mu]$, \
$CellContext`\[Sigma]$} = 
         If[$CellContext`STAT$$ == 
           1, {$CellContext`\[Mu]$, $CellContext`\[Sigma]$}, {$CellContext`\
\[Mu]$ = Mean[$CellContext`S$], $CellContext`\[Sigma]$ = 
            StandardDeviation[$CellContext`S$]}]; $CellContext`FPC$ = 
         If[$CellContext`POP$$ == 1, 
           Sqrt[1. - $CellContext`nSize$$/$CellContext`NX], 
           1]; $CellContext`gN$ = Plot[
           PDF[
            
            NormalDistribution[$CellContext`\[Mu]$, $CellContext`\[Sigma]$ \
$CellContext`FPC$], $CellContext`z], {$CellContext`z, $CellContext`\[Mu]$ - 
            3 $CellContext`\[Sigma]$, $CellContext`\[Mu]$ + 
            3 $CellContext`\[Sigma]$}, Axes -> False, Frame -> True, 
           PlotStyle -> {Red}, PlotRange -> $CellContext`PR$, 
           ImagePadding -> $CellContext`padding]; $CellContext`gS$ = 
         Histogram[$CellContext`S$, "Wand", "ProbabilityDensity", 
           PlotLabel -> $CellContext`PLS$, Axes -> False, Frame -> True, 
           PlotRange -> $CellContext`PR$, ChartStyle -> Opacity[0.5], 
           ImagePadding -> $CellContext`padding]; $CellContext`epi$ = Text[
           Style[
            Row[{
              Style["N", Italic], "(", 
              NumberForm[$CellContext`\[Mu]$, {5, 1}], ", ", 
              NumberForm[$CellContext`FPC$ $CellContext`\[Sigma]$, {5, 1}], 
              ")"}], 10, Red], 
           Scaled[{0.15, 0.8}]]; $CellContext`GS$ = 
         Show[{$CellContext`gS$, $CellContext`gN$}, 
           PlotRange -> $CellContext`PR$, Epilog -> $CellContext`epi$, 
           PlotLabel -> $CellContext`PLS$, 
           ImagePadding -> $CellContext`padding]; GraphicsGrid[{{
            
            Which[$CellContext`POP$$ == 
             1, $CellContext`gH, $CellContext`POP$$ == 
             2, $CellContext`pdfnor, $CellContext`POP$$ == 
             3, $CellContext`pdfuni, $CellContext`POP$$ == 
             4, $CellContext`pdfL, 
             True, $CellContext`pdfG]}, {$CellContext`GS$}}, ImageSize -> 
          350]], "Specifications" :> {{{$CellContext`POP$$, 1, 
          "population"}, {
         1 -> "finite", 2 -> "normal", 3 -> "uniform", 4 -> "Laplace", 5 -> 
          "gamma"}, ControlPlacement -> 
         Top}, {{$CellContext`STAT$$, 1, "statistic"}, {
         1 -> "mean", 2 -> "standard deviation"}, ControlPlacement -> 
